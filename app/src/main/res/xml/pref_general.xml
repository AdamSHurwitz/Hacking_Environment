        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:summary="@string/asyncparcelable2_settings_summary"
        android:title="@string/asyncparcelable2_settings_title"></ListPreference>
    <ListPreference
        android:defaultValue="popular"
        android:entries="@array/asynccursor1_settings_entries"
        android:entryValues="@array/asynccursor1_settings_values"
        android:key="@string/asynccursor1_settings_key"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:summary="@string/asynccursor1_settings_summary"
        android:title="@string/asynccursor1_settings_title"></ListPreference>
    <ListPreference
        android:defaultValue="popular"
        android:entries="@array/asynccursor2_settings_entries"
        android:entryValues="@array/asynccursor2_settings_values"
        android:key="@string/asynccursor2_settings_key"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:summary="@string/asynccursor2_settings_summary"
        android:title="@string/asynccursor2_settings_title"></ListPreference>


</PreferenceScreen>
